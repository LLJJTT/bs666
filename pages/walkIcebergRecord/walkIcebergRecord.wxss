.walk_record{
	height: 100%;
}
.swiper_box{
	width: 100%;
	height: 100%;
	background-size:cover;
	background-repeat: no-repeat;
	background-position: center center;
}
swiper{
  height:100%;
  width: 100%;
  position: absolute;
  top: 0;
}
.swiper-image {
  height: 100%;
  width: 100%;
}
swiper-item{
	height: 100%;
}
.bg{
	width: 100%;
	height: 100%;
}
.huadong_content{
	height: 100%;
	background-size:532rpx 800rpx;
	background-repeat: no-repeat;
	background-position: center center;
}
/* 左右切换 */
.tab{
  width: 100%;
  position: fixed;
  top:60rpx;
  z-index: 9999;

}
.r_l_date{
	width: 90%;
	margin:0 auto;
	display: inline-block;
	color: #fff;
	font-size: 36rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}
.r_l_date image{
	display: inline-block;
	width: 40rpx;
	height: 40rpx;
  	position: relative;
	vertical-align: middle;
	padding: 10rpx;
}
.r_l_date .time_date{
	display: inline-block;
	color: #fff;
	margin:0 100rpx;
}
/* 点 */
.dot_fa{
	display: flex;
	align-items: center;
	justify-content: center;
	margin-top: 30rpx;
}
.dot{
	float: left;
	width: 10rpx;
	height: 10rpx;
	border-radius: 50%;
	background: #fff;
}
.active{
	background: #dd6f72;
}
.dot:not(:first-child){
	margin-left: 20rpx;
}
/* 记录 -盒子 */
.content_box{
	/* background: blue; */
	position: relative;
	margin-top: 250rpx;
	max-height: 750rpx;
	overflow: auto;
}
.content_box view{
	overflow-x: auto;
}
/* 事件 */
.event{
	color: #fff;
	float: right;
	width:410rpx;
}
.e_word{
	display: inline-block;
	width: 70rpx;
	height: 70rpx;
	line-height: 70rpx;
	text-align: center;
	font-size: 28rpx;
	background:#fff;
	color: #ff6f72;
	border-radius: 50%;
	border:2rpx solid #ff6f72;
	float: left;
}
.e_content{
	max-width: 70%;
	float: left;
	margin-left: 10rpx;
	padding: 10rpx 20rpx;
	border:2rpx solid #dd6f72;
	border-radius: 20rpx;
	background: #fff;
	font-size: 24rpx;
	color: #dd6f72;
}
